<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item app:state_raised="true">
        <set>
            <objectAnimator android:interpolator="@android:interpolator/fast_out_slow_in" android:duration="@android:integer/config_mediumAnimTime" android:valueTo="@dimen/app_bar_elevation" android:valueType="floatType" android:propertyName="translationZ" />
            <objectAnimator android:duration="@android:integer/config_mediumAnimTime" android:valueTo="0.9" android:valueType="floatType" android:propertyName="alpha" />
        </set>
    </item>
    <item>
        <set>
            <objectAnimator android:interpolator="@android:interpolator/fast_out_slow_in" android:duration="@android:integer/config_mediumAnimTime" android:valueTo="0.0dip" android:valueType="floatType" android:propertyName="translationZ" />
            <objectAnimator android:duration="@android:integer/config_mediumAnimTime" android:valueTo="1" android:valueType="floatType" android:propertyName="alpha" />
        </set>
    </item>
</selector>